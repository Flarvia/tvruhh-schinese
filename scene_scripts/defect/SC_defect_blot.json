{
	"script" : [
		{
			"use_textbox"		: 1,
			"play_music"		: "green_scene_music",
			"bg_image"			: "spr_scene_raining_outside",
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_sad"],
			"dialog"			: "We are so sorry.|L||!|Did we get any paint on you, cutie?",
		},
		{
			"character"			: ["VOICE_defect", "CHAR_defect_left", "MOOD_confused"],
			"dialog"			: "No, I don't think so.",
		},
		{
			"character"			: ["VOICE_defect", "CHAR_defect_left", "MOOD_awkward"],
			"dialog"			: "I wouldn't mind either way.",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_shocked"],
			"dialog"			: "Oh!",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_happy"],
			"dialog"			: "Don't tempt us to cover you in paint, cutie.",
		},
		{
			"character"			: ["VOICE_defect", "CHAR_defect_left", "MOOD_blush"],
			"dialog"			: "Oh... I-",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_content"],
			"dialog"			: "We're only joking, cutie.",
		},
		{
			"character"			: ["VOICE_defect", "CHAR_defect_left", "MOOD_confused_blush"],
			"dialog"			: "...",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_happy"],
			"dialog"			: "|wave|Look at this cute little Veyeral!",
		},
		{
			"character"			: ["VOICE_veyeral", "CHAR_blue_veyeral_left", "MOOD_happy"],
			"dialog"			: "Ah yes, your eyes must have naturally been attracted towards me.",
		},
		{
			"character"			: ["VOICE_veyeral", "CHAR_blue_veyeral_left", "MOOD_happy"],
			"dialog"			: "There is nothing I can do about it.|L||!|We just have to live with the consequences.",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_normal"],
			"dialog"			: "We have to paint the both of you, cuties!",
		},
		{
			"character"			: ["VOICE_defect", "CHAR_defect_left", "MOOD_confused_blush"],
			"dialog"			: "Umm...",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_content"],
			"dialog"			: "We mean make a painting of you.|L||!|Not cover you in paint.",
		},
		{
			"character"			: ["VOICE_defect", "CHAR_defect_left", "MOOD_surprised"],
			"dialog"			: "Oh!",
		},
		{
			"character"			: ["VOICE_veyeral", "CHAR_blue_veyeral_left", "MOOD_happy"],
			"dialog"			: "|wave|My likeness captured in a painting forever?",
		},
		{
			"character"			: ["VOICE_veyeral", "CHAR_blue_veyeral_left", "MOOD_normal"],
			"dialog"			: "Do they make paint blue enough for that?",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_happy"],
			"dialog"			: "Yeah!|L||!|They actually make every color blue enough for you!",
		},
		{
			"character"			: ["VOICE_veyeral", "CHAR_blue_veyeral_left", "MOOD_happy"],
			"dialog"			: "|wave|Well it's about time the other colors caught up.",
		},
		{
			"character"			: ["VOICE_defect", "CHAR_defect_left", "MOOD_confused"],
			"dialog"			: "Are we going to go see your art gallery?|L||!|I think you invited me in a dream.",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_happy"],
			"dialog"			: "We sure did, cutie!",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_normal"],
			"dialog"			: "It's not that far from here.",
		},
		{
			"character"			: ["VOICE_defect", "CHAR_defect_left", "MOOD_happy"],
			"dialog"			: "Then let's go!",
		},
		{
			"character"			: ["VOICE_veyeral", "CHAR_blue_veyeral_left", "MOOD_normal"],
			"dialog"			: "You should probably warn the other guests of my arrival.",
		},
		{
			"character"			: ["VOICE_veyeral", "CHAR_blue_veyeral_left", "MOOD_happy"],
			"dialog"			: "We wouldn't want anyone to mistake me for one of the masterpieces on display.",
		},
		{
			"character"			: ["VOICE_blot", "CHAR_blot_right", "MOOD_very_happy"],
			"dialog"			: "We'll get right on that, cutie!",
		},
	]
}